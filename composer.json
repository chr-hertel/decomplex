{
    "name": "stoffel/decomplex",
    "description": "Slim web interface to diff complexity of code snippets",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "niels-de-blaauw/php-doc-check": "^0.2.2",
        "pugx/shortid-php": "^0.7.0",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/flex": "^1.17",
        "symfony/framework-bundle": "6.0.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/runtime": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "6.0.*",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.25.3",
        "phpmd/phpmd": "^2.10",
        "pdepend/pdepend": "2.x-dev",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "spatie/phpunit-snapshot-assertions": "^4.1",
        "symfony/browser-kit": "6.0.*",
        "symfony/css-selector": "6.0.*",
        "symfony/debug-bundle": "6.0.*",
        "symfony/phpunit-bridge": "6.0.*",
        "symfony/stopwatch": "6.0.*",
        "symfony/var-dumper": "6.0.*",
        "symfony/web-profiler-bundle": "6.0.*",
        "vimeo/psalm": "^4.12.0"
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "phpunit/phpunit": "^9.5.10",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*",
            "docker": false
        }
    }
}
