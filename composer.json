{
    "name": "stoffel/decomplex",
    "description": "Slim web interface to diff complexity of code snippets",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "niels-de-blaauw/php-doc-check": "^0.2.2",
        "pugx/shortid-php": "^0.6.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/twig-bundle": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.1.*",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "infection/infection": "^0.16.3",
        "phpmd/phpmd": "^2.8",
        "phpstan/phpstan": "^0.12.23",
        "phpstan/phpstan-phpunit": "^0.12.8",
        "sensiolabs/security-checker": "^6.0",
        "spatie/phpunit-snapshot-assertions": "^4.1",
        "symfony/browser-kit": "5.1.*",
        "symfony/css-selector": "5.1.*",
        "symfony/debug-bundle": "5.1.*",
        "symfony/phpunit-bridge": "5.1.*",
        "symfony/stopwatch": "5.1.*",
        "symfony/var-dumper": "5.1.*",
        "symfony/web-profiler-bundle": "5.1.*",
        "vimeo/psalm": "^3.11"
    },
    "config": {
        "platform": {
            "php": "7.4.6"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "phpunit/phpunit": "^9.1.4",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
